{
    "name": "OneNoteWeb4ChromeOS",
    "version": "0.1.0",
    "description": "Fixes pen input in OneNote Web on ChromeOS",
    "host_permissions": ["https://*.live.com/*"],
    "content_scripts": [
    {
      "matches": ["https://*.live.com/*"],
      "run_at": "document_start",
      "js": ["contentScript.js"],
      "all_frames": true
    }
    ],
    "manifest_version": 3
}
